{
    "terminal.integrated.env.linux": {
        "EXT_PATH": "${workspaceFolder}/build/install/bin:${workspaceFolder}/third-party/flatbuffers/cmake-out:${workspaceFolder}/_demos/bin",
        "EXT_PYTHONPATH": "${workspaceFolder}",
    },
    "terminal.integrated.env.osx": {
        "EXT_PATH": "${workspaceFolder}/build/install/bin:${workspaceFolder}/third-party/flatbuffers/cmake-out:${workspaceFolder}/_demos/bin",
        "EXT_PYTHONPATH": "${workspaceFolder}",
    },
    "cmake.configureSettings": {
        "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/build/install",
        "BUCK2": "buck2",
        // "ET_MIN_LOG_LEVEL": "Debug",
    },
    "cmake.buildDirectory": "${workspaceFolder}/cmake-out",
    "clangd.arguments": [
        // "-log=verbose",
        "-pretty",
        "--background-index",
        "--enable-config",
        "--all-scopes-completion",
        "--compile-commands-dir=${workspaceFolder}/cmake-out"
    ],
    "python.analysis.extraPaths": [
        // "${workspaceFolder}",
    ],
    "python.analysis.exclude": [
        "${workspaceFolder}",
    ],
    // "python.analysis.autoImportCompletions": true,
    // "python.analysis.typeCheckingMode": "basic"
    // "C_Cpp.intelliSenseEngine": "default",
    // "C_Cpp.intelliSenseCachePath": "${workspaceFolder}/build/.vsc_cpp_cache",
